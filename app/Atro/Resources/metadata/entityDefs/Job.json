{
  "fields": {
    "name": {
      "type": "varchar",
      "required": true
    },
    "status": {
      "type": "enum",
      "options": [
        "Pending",
        "Running",
        "Success",
        "Failed",
        "Canceled"
      ],
      "default": "Pending"
    },
    "pid": {
      "type": "int"
    },
    "executeTime": {
      "type": "datetime",
      "required": true
    },
    "priority": {
      "type": "float",
      "default": 100
    },
    "handler": {
      "type": "varchar",
      "readOnly": true
    },
    "payload": {
      "type": "jsonObject",
      "readOnly": true
    },
    "serviceName": {
      "type": "varchar",
      "required": true,
      "maxLength": 100
    },
    "method": {
      "type": "varchar",
      "required": true,
      "maxLength": 100
    },
    "methodName": {
      "type": "varchar",
      "maxLength": 100
    },
    "data": {
      "type": "jsonObject"
    },
    "scheduledJob": {
      "type": "link"
    },
    "scheduledJobJob": {
      "type": "foreign",
      "link": "scheduledJob",
      "field": "job"
    },
    "attempts": {
      "type": "int"
    },
    "targetId": {
      "type": "varchar",
      "maxLength": 48
    },
    "targetType": {
      "type": "varchar",
      "maxLength": 64
    },
    "failedAttempts": {
      "type": "int"
    },
    "queueItem": {
      "type": "link"
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    }
  },
  "links": {
    "queueItem": {
      "type": "belongsTo",
      "entity": "QueueItem"
    },
    "scheduledJob": {
      "type": "belongsTo",
      "entity": "ScheduledJob"
    }
  },
  "collection": {
    "sortBy": "createdAt",
    "asc": false,
    "textFilterFields": [
      "name",
      "methodName",
      "serviceName",
      "scheduledJobName"
    ]
  },
  "indexes": {
    "executeTime": {
      "columns": [
        "status",
        "executeTime"
      ]
    },
    "status": {
      "columns": [
        "status",
        "deleted"
      ]
    }
  }
}
