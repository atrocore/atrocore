{
  "fields": {
    "name": {
      "type": "varchar",
      "view": "views/file/fields/name",
      "notNull": true,
      "required": true,
      "trim": true,
      "layoutUploadDisabled": true
    },
    "description": {
      "type": "text"
    },
    "preview": {
      "type": "varchar",
      "view": "views/file/fields/preview",
      "notStorable": true,
      "layoutDetailDisabled": true,
      "layoutListDisabled": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true,
      "exportDisabled": true
    },
    "hidden": {
      "type": "bool",
      "notNull": true,
      "default": false
    },
    "tags": {
      "type": "extensibleMultiEnum",
      "extensibleEnumId": "fileTags",
      "dropdown": false
    },
    "url": {
      "type": "varchar",
      "notStorable": true,
      "readOnly": true,
      "layoutDetailDisabled": true,
      "layoutListDisabled": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": false,
      "exportDisabled": true
    },
    "hasOpen": {
      "type": "bool",
      "notStorable": true,
      "layoutDetailDisabled": true,
      "layoutListDisabled": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true,
      "exportDisabled": true
    },
    "extension": {
      "type": "varchar",
      "notStorable": true,
      "readOnly": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true
    },
    "downloadUrl": {
      "type": "varchar",
      "notStorable": true,
      "readOnly": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true
    },
    "smallThumbnailUrl": {
      "type": "varchar",
      "notStorable": true,
      "readOnly": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true
    },
    "mediumThumbnailUrl": {
      "type": "varchar",
      "notStorable": true,
      "readOnly": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true
    },
    "largeThumbnailUrl": {
      "type": "varchar",
      "notStorable": true,
      "readOnly": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true
    },
    "mimeType": {
      "type": "varchar",
      "readOnly": true,
      "layoutMassUpdateDisabled": true,
      "importDisabled": true
    },
    "fileSize": {
      "type": "int",
      "readOnly": true,
      "layoutMassUpdateDisabled": true,
      "importDisabled": true,
      "min": 0
    },
    "fileMtime": {
      "type": "datetime",
      "readOnly": true,
      "layoutMassUpdateDisabled": true,
      "importDisabled": true
    },
    "hash": {
      "type": "varchar",
      "readOnly": true,
      "index": true,
      "layoutMassUpdateDisabled": true,
      "importDisabled": true
    },
    "storage": {
      "type": "link",
      "readOnly": true
    },
    "folder": {
      "type": "link",
      "view": "views/file/fields/folder",
      "required": false
    },
    "folderPath": {
      "type": "jsonObject",
      "view": "views/folder/fields/folder-path",
      "notStorable": true,
      "readOnly": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true,
      "emHidden": true
    },
    "path": {
      "type": "varchar",
      "maxLength": 300,
      "view": "views/fields/varchar",
      "readOnly": true,
      "default": null,
      "layoutMassUpdateDisabled": true,
      "importDisabled": true
    },
    "thumbnailsPath": {
      "type": "text",
      "view": "views/fields/varchar",
      "readOnly": true,
      "default": null,
      "layoutMassUpdateDisabled": true,
      "importDisabled": true
    },
    "type": {
      "type": "link",
      "view": "views/file/fields/type",
      "required": false
