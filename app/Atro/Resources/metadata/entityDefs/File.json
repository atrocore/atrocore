    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "extensionCorrected": {
      "type": "bool",
      "notStorable": true,
      "layoutDetailDisabled": true,
      "layoutListDisabled": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true,
      "exportDisabled": true
    },
    "width": {
      "type": "float",
      "measureId": "dimension",
      "readOnly": true
    },
    "height": {
      "type": "float",
      "measureId": "dimension",
      "readOnly": true
    },
    "size": {
      "type": "varchar",
      "measureId": "dimension",
      "readOnly": true,
      "notStorable": true,
      "filterDisabled": true,
      "importDisabled": true,
      "exportDisabled": true
    },
    "colorSpace": {
      "type": "varchar",
      "readOnly": true
    },
    "scanId": {
      "type": "varchar",
      "len": 36,
      "readOnly": true,
      "layoutDetailDisabled": true,
      "layoutListDisabled": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "importDisabled": true,
      "exportDisabled": true
    }
  },
  "links": {
    "storage": {
      "type": "belongsTo",
      "foreign": "files",
      "entity": "Storage"
    },
    "folder": {
      "type": "belongsTo",
      "foreign": "files",
      "entity": "Folder"
    },
    "type": {
      "type": "belongsTo",
      "foreign": "files",
      "entity": "FileType"
    },
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    }
  },
  "collection": {
    "sortBy": "fileMtime",
    "asc": false
  },
  "uniqueIndexes": {
    "unique_file": [
      "deleted",
      "name",
      "path",
      "storage_id"
    ]
  }
}
