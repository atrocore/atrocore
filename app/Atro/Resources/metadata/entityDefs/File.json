    "uploadUrl": {
      "type": "varchar",
      "view": "views/file/fields/upload-url",
      "notStorable": true,
      "layoutListDisabled": true,
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "importDisabled": true,
      "exportDisabled": true,
      "filterDisabled": true
