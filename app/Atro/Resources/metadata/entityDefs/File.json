    "files": {
      "type": "varchar",
      "notStorable": true,
      "view": "views/file/fields/upload",
      "layoutListDisabled": true,
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "importDisabled": true,
      "exportDisabled": true,
      "filterDisabled": true
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
