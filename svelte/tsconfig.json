{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "lib": [
      "DOM",
      "ES2018"
    ],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "svelte"
    ],
    "paths": {
      "$lib/*": [
        "src/lib/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "__sapper__"
  ]
}